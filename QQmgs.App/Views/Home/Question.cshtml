@model int

@{
    Layout = "~/Views/Shared/_Materize_Layout.cshtml";
}


@switch (Model)
{
    case 0:
        Html.RenderPartial("Question_sub_0_View");
        break;
    case 1:
        Html.RenderPartial("Question_sub_1_View");
        break;
    case 2:
        Html.RenderPartial("Question_sub_2_View");
        break;
    case 3:
        Html.RenderPartial("Question_sub_3_View");
        break;
    case 4:
        Html.RenderPartial("Question_sub_4_View");
        break;
    default:
        Html.RenderPartial("Question_sub_0_View");
        break;
}